{
  "name": "Chelewonder",
  "version": "0.5",
  "description": "Querés ver tu menú? Tomá wachin",
  "manifest_version": 2,
  "author": "maeriens",
  "browser_action": {
    "default_icon": "128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://w1131323.ferozo.com/wonderfood/Pedidos*"
      ],
      "js": [
        "js/pedidos.js"
      ]
    },
    {
      "matches": [
        "http://w1131323.ferozo.com/wonderfood/Productos*"
      ],
      "js": [
        "js/productos.js"
      ]
    }
  ]
}